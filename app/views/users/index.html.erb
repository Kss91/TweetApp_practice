<%= stylesheet_link_tag "posts", "data-turbo-track": "reload" %>
<div class="container">
  <h1>Users#index</h1>
  <p>Find me in app/views/users/index.html.erb</p>
  <div class="posts-index">
    <% @users.each do |user| %>
      <div class="post-user">
        <img src="<%= "/user_images/default_img.jpg"%>">
      </div>
      <div class="posts-index-items">
        <%= link_to(user.name, "/users/#{user.id}") %>
        <p><%= user.email %></p>
      </div>
    <% end %>
  </div>
</div>

